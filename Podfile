# Uncomment the next line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'

target 'QuickReport' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for QuickReport

pod 'Alamofire'
pod 'ObjectMapper'
pod 'AlamofireObjectMapper'
pod 'SwiftyJSON'
pod 'RealmSwift'
pod 'mailcore2-ios'
pod 'KeyClip'
pod 'Eureka'

  target 'QuickReportTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'QuickReportUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
